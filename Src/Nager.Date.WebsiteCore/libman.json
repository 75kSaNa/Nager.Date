{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "jquery@3.2.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.2.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "vue@2.5.22",
      "destination": "wwwroot/lib/vue/"
    },
    {
      "provider": "cdnjs",
      "library": "axios@0.18.0",
      "destination": "wwwroot/lib/axios/"
    },
    {
      "provider": "cdnjs",
      "library": "moment.js@2.23.0",
      "destination": "wwwroot/lib/moment/"
    },
    {
      "provider": "unpkg",
      "library": "font-awesome-5-css@5.4.1",
      "destination": "wwwroot/lib/font-awesome/"
    }
  ]
}